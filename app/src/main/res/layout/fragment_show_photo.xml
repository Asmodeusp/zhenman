<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/h1"
    tools:context=".view.home.ShowPhotoFragment">

    <ImageView
        android:id="@+id/background"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/h1" />

    <me.panpf.sketch.SketchImageView
        android:id="@+id/show_photo_ImageView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_centerInParent="true"
        android:background="@mipmap/common_qiezi"
        android:backgroundTint="@color/h1" />

</com.zhy.autolayout.AutoRelativeLayout>